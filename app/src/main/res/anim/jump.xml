<?xml version="1.0" encoding="utf-8"?>
<set xmlns:android="http://schemas.android.com/apk/res/android"
    android:shareInterpolator="false" android:repeatMode="restart">
    <set android:duration="200">
        <translate android:toYDelta="-200" />
    </set>

    <set android:duration="200" android:startOffset="200">
        <translate android:toYDelta="-150" />
    </set>

    <set android:duration="200" android:startOffset="400">
        <translate android:toYDelta="-100"/>
    </set>

    <set android:duration="200" android:startOffset="600">
        <translate android:toYDelta="0" android:toXDelta="0"/>
    </set>
</set>
